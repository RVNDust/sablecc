Macro class
    Param
        name : String;
        fields : field, getter, setter, separator="\n";
        methods : method, separator="\n";
{Begin}
{Insert: copyright}
class {name}{
{fields}
{methods}
}

{End}

Macro method
    Param
        name : String;
        access : String;
        returnType : String, none="void";
        args : param, separator=", ";
{Begin}
{access} {returnType} {name} ({args}){
    bodyMethod
}
{End}

Macro param
    Param
        name : String;
        type : String;
{Begin}
{type} {name}
{End}

Macro field
    Param
        name : String;
        type : String;
{Begin}
{type} {name};

{End}

Macro getter
    Param
        name : String;
        returnType : String;
{Begin}
public {returnType} get{name}(){
   return this.{name};
}
{End}

Macro setter
    Param
        name : String;
        arg : param;
{Begin}
public void set{name}({arg}){
}
{End}
